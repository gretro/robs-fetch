{
  "name": "robs-fetch",
  "version": "0.1.0",
  "description": "Redux-Observable fetch",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm test && npm run build:src",
    "build:src": "tsc",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gretro/robs-fetch.git"
  },
  "author": "Gabriel Lemire",
  "keywords": [
    "redux",
    "rxjs",
    "redux-observable",
    "fetch"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gretro/robs-fetch/issues"
  },
  "homepage": "https://github.com/gretro/robs-fetch#readme",
  "dependencies": {
    "redux": "^3.0.0",
    "rxjs": "^5.0.0"
  },
  "peerDependencies": {
    "redux": "^3.0.0",
    "rxjs": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "jest": "^19.0.2",
    "rimraf": "^2.6.1",
    "ts-jest": "^19.0.6",
    "typescript": "^2.2.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
